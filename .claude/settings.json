{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "extraKnownMarketplaces": {
    "duck-soup": {
      "source": {
        "source": "github",
        "repo": "schuyler/duck-soup"
      }
    }
  },
  "enabledPlugins": {
    "dev@duck-soup": true
  },
  "includeCoAuthoredBy": false,
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "./.claude/scripts/install-gh.sh"
          }
        ]
      }
    ]
  }
}
